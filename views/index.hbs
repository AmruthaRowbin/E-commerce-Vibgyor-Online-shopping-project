                <div class="row">
                    <div class="col-lg-12">
                        <h1>2023</h1>
                        {{!-- <h2>Fasion World</h2> --}}
                        <h2>{{banner.heading}}.</h2>
                        <a href="/shop" class="primary-btn">See Products</a>
                    </div>
                </div>
            </div>
        </div>

</section>
<!-- Hero Slider End -->


