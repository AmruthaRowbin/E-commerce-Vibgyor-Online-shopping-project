<!-- Lookbok Section Begin -->
<!-- Header Info Begin -->
<div class="header-info">
    <div class="row">
        {{#each coupons}}
        {{#if this.deactivate}}
        {{else if this.expired}}
        {{else}}
        <div class="col-12 text-left text-lg-center">
            <div class="header-item d-flex align-items-center justify-content-center">
                <img src="img/icons/voucher.png" alt="">
                <p>Coupon Code: {{code}} ({{discount}} Discount)</p>
            </div>
        </div>
        {{/if}}
        {{/each}}
    </div>
</div>
<!-- Header Info End -->
<!-- Header End -->

<!-- Hero Slider Begin -->
<section class="hero-slider">
    <div class="hero-items owl-carousel">
        <div class="single-slider-item set-bg" data-setbg="{{banner.image}}">
